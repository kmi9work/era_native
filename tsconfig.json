{
  "extends": "@react-native/typescript-config",
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["**/node_modules", "**/Pods"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@registry": ["./src/registry"],
      "@config/*": ["./src/config/*"],
      "@plugins/*": ["./src/plugins/*"],
      "@game-plugins/*": ["./src/plugins/game-plugins/*"]
    }
  }
}
